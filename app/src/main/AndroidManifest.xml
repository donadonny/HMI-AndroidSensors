<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sk.stuba.fei.hmi_androidsensors">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HMIActivity"></activity>
        <activity android:name=".Accelerometer.AccelerometerActivity"></activity>
        <activity android:name=".LightSensor.LightSensorActivity"></activity>
        <activity android:name=".MagneticFieldSensor.MagneticFieldActivity"></activity>
        <activity android:name=".ProximitySensor.ProximitySensorActivity"></activity>
    </application>

</manifest>
